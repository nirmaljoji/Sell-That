    <main>
        
        <section class="portfolio-block block-intro">
            <!--Rakshana Start-->
            
                <div class="row">
                    <div class="col-md-5 textArea">
                        <h2>
                            Wasaap fellow Amrita dudes & bros!
                        </h2>
                        <p>
                            A one stop website for all Amrita students to interact, sell used goods and report lost & found items!

                        </p>

                        <div class="loginContainer">
                            <div class="row">
                                
                                
                                     <a class="custom-btn" data-toggle="modal" href="javascript:void(0)" onclick="openLoginModal();">Log in</a>
                                     <a class="custom-btn" data-toggle="modal" href="javascript:void(0)" onclick="openRegisterModal();">Register</a>
                               
                            </div>
                      </div>
                    </div>
                </div>
                <div class="col-md-2"></div>
                <!--    
                <div class="col-md-5 bg-image-home">
