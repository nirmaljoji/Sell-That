  <div class="page-wrapper">
    <div class="headpart">
      <h2 id="found">Found items near you</h2>
      <button class=" btn float-right postBtn" id="postbtn">Your items</button>
      <div id="postItemModal" class=modal>
        <div class="modal-content">
          <span class="close">&#10005;</span>
          <div class="tab">
            <button class="tablinks" onclick="openTab(event,'tab1')">Post an item you found</button>
            <button class="tablinks" onclick="openTab(event,'tab2')">Previous items</button>
          </div>
          <div id="tab1" class="tabcontent">
            <h4>Fill in the details</h4>
            <h6>Please add the information for other students to view before submitting the item to the Admin
              Department.</h6>
            <br>
            <div class="formDiv">
              <form action="/lostFound" method="POST">
                <label for="fname">Item name</label>
                <input type="text" id="fname" required name="item_name" placeholder="Item name">

                <label for="lname">Found in</label>
                <input type="text" id="lname" required name="item_place" placeholder="Location found">

                <label for="details">Note</label>
                <input type="text" id="details" name="item_desc" placeholder="Add any relevant details..">
                <hr>
                <div>
                  <label for="upload" class="uploadButton">Upload image</label>
                  <input type="file" name="upload" id="upload" required>
                </div>
                <br>
                <input type="submit" value="Submit" onclick="lostFoundForm()">
              </form>
            </div>
          </div>

          <div id="tab2" class="tabcontent">

            <div class="prev">
              <h5>Debit Card</h5>
              <p>Found near reception</p>
              <form action="/delLostAndFound" method="POST">
                <input type="submit" class="btn btn-primary del">&#10005;</input>
              </form>

            </div>


          </div>

          <div id="tab2" class="tabcontent">
            <p>prev</p>

          </div>
        </div>
      </div>
      <hr class="headLine">
    </div>
